<table>
    <tr>
        <th></th>
        <% for (var c=0; c<w.titleCol.length; c++) { %>
            <th><%= w.titleCol[c] %></th>
        <% } %>
    </tr>
    <% for (var r=0; r<w.titleRow.length; r++) { %>
    <tr>
        <th><%= w.titleRow[r] %></th>
        <% for (var c=0; c<w.titleCol.length; c++) { %>
            <th><input type="number" name="<%= prev || (w.titleRow[r] + '-' + w.titleCol[c]) %>"
                       id="<%= w.titleRow[r] + '-' + w.titleCol[c] %><%= prev %>"
                       value="<%= model[w.titleRow[r] + '-' + w.titleCol[c]] || 0%>" step="0.01" min="0" max="100"></th>
        <% } %>
    </tr>
    <% } %>
</table>